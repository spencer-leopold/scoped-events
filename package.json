{
  "name": "scoped-events",
  "version": "1.0.1",
  "description": "Small event library that lets you namespace/scope your events",
  "main": "./index.js",
  "scripts": {
    "jshint": "jshint ./lib/*.js index.js --verbose",
    "test": "mocha -R spec ./test --recursive",
    "coverage": "istanbul cover _mocha -- -R spec ./test --recursive",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/spencer-leopold/scoped-events.git"
  },
  "keywords": [
    "events",
    "dispatcher",
    "scoped",
    "namespaced"
  ],
  "author": "Spencer Leopold <spencer.leopold@gmail.com>",
  "bugs": {
    "url": "https://github.com/spencer-leopold/scoped-events/issues"
  },
  "homepage": "https://github.com/spencer-leopold/scoped-events#readme",
  "devDependencies": {
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.15",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "sinon": "^1.15.3",
    "sinon-chai": "^2.8.0"
  }
}
